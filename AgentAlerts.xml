<EnterpriseApplication>
	<ear-info>
		<ear-name>AgentAlerts.ear</ear-name>
		<app-name>AgentAlerts</app-name>
	</ear-info>
	<mod-info>
		<module>
			<web>
				<war-file-name>AgentAlertsWeb.war</war-file-name>
				<display-name>AgentAlertsWeb</display-name>
				<virtual-host>default_host</virtual-host>
				<server-name>appsint-eCalypso-com</server-name>
			</web>
		</module>
		<module>
			<ejb>
				<ejb-file-name>AgentAlertsEJB.jar</ejb-file-name>
				<display-name>AgentAlertsEJB</display-name>
			</ejb>
		</module>
		<module>
			<ejb>
				<ejb-file-name>AgentAlertsListener.jar</ejb-file-name>
				<display-name>AgentAlertsListener</display-name>
			</ejb>
		</module>
	</mod-info>
	<Security>
		<J2CAuthData>
			<J2C>
				<Alias>AgentAlerts_SQL_IntellAgent</Alias>
				<Username>IntellAgent_Websphere_user</Username>
				<Password>iawU#@12</Password>
			</J2C>
			<J2C>
				<Alias>AgentAlerts_SONIC_CF</Alias>
				<Username>KAHAgentAlerts</Username>
				<Password>D23yY017</Password>
			</J2C>
		</J2CAuthData>
	</Security>
	<Node>
		<NodeName>was730aINT64V70</NodeName>
		<NodeName>was730bINT64V70</NodeName>
	</Node>
	<ApplicationServer>
		<ClusterName>AgentAlerts</ClusterName>
		<Instances>
			<Server>
				<EndPoints>
					<PortItem>
						<PortName>WC_defaulthost</PortName>
						<Port>9134</Port>
					</PortItem>
				</EndPoints>				
				<WebContainer>
					<ServletCaching>NO</ServletCaching>
					<!-- add this custom property to disable Websphere from automatically
					appending "Cache-Control" headers to responses which breaks the display of PDFs
					in IE when we use https; this works in IE for http and works in FireFox for both
					http and https. 
					Web containter transport chains -> WCInboundDefault -> HTTP inbound channel
					<CustomProperties>
						<Property>
							<Name>CookiesConfigureNoCache</Name>
							<Value>False</Value>
						</Property>
					</CustomProperties>
					-->
					<SessionManagement>
						<CookieName>AAJSESSIONID</CookieName>
						<SessionTimeout>30</SessionTimeout>
						<DistributedSessionType>NONE</DistributedSessionType>
					</SessionManagement>
				</WebContainer>
				<JavaVirtualMachine>
					<VerboseGarbageCollection>TRUE</VerboseGarbageCollection>
					<InitialHeapSize>32</InitialHeapSize>
					<MaximumHeapSize>256</MaximumHeapSize>
				</JavaVirtualMachine>
				<MonitoringPolicy>
					<maximumStartupAttempts>3</maximumStartupAttempts>
					<pingInterval>60</pingInterval>
					<pingTimeout>300</pingTimeout>
					<autoRestart>true</autoRestart>
					<serverRestartState>RUNNING</serverRestartState>
				</MonitoringPolicy>
				<JVMLogs>
					<SystemOut>
						<JVMLogsSystemOutFileFormat>BASIC</JVMLogsSystemOutFileFormat>
						<JVMLogsSystemOutRolloverType>BOTH</JVMLogsSystemOutRolloverType>
						<JVMLogsSystemOutNumberOfBackupFiles>5</JVMLogsSystemOutNumberOfBackupFiles>
						<JVMLogsSystemOutRolloverSize>50</JVMLogsSystemOutRolloverSize>
						<JVMLogsSystemOutBaseHour>24</JVMLogsSystemOutBaseHour>
						<JVMLogsSystemOutRolloverPeriod>24</JVMLogsSystemOutRolloverPeriod>
						<JVMLogsSystemOutFormatWrites>true</JVMLogsSystemOutFormatWrites>
						<JVMLogsSystemOutsuppresWrites>false</JVMLogsSystemOutsuppresWrites>
						<JVMLogsSystemOutsuppressStackTrace>false</JVMLogsSystemOutsuppressStackTrace>
					</SystemOut>
					<SystemError>
						<JVMLogsSystemErrorFileFormat>BASIC</JVMLogsSystemErrorFileFormat>
						<JVMLogsSystemErrorRolloverType>BOTH</JVMLogsSystemErrorRolloverType>
						<JVMLogsSystemErrorNumberOfBackupFiles>5</JVMLogsSystemErrorNumberOfBackupFiles>
						<JVMLogsSystemErrorRolloverSize>50</JVMLogsSystemErrorRolloverSize>
						<JVMLogsSystemErrorBaseHour>24</JVMLogsSystemErrorBaseHour>
						<JVMLogsSystemErrorRolloverPeriod>24</JVMLogsSystemErrorRolloverPeriod>
						<JVMLogsSystemErrorFormatWrites>true</JVMLogsSystemErrorFormatWrites>
						<JVMLogsSystemErrorsuppresWrites>false</JVMLogsSystemErrorsuppresWrites>
						<JVMLogsSystemErrorsuppressStackTrace>false</JVMLogsSystemErrorsuppressStackTrace>
					</SystemError>
				</JVMLogs>
				<MessageListenerService>
					<ListenerPorts>
						<Port>
							<Name>IntellAgentListenerPort</Name>
							<CFJNDI>jms/IAConnectionFactory</CFJNDI>
							<DestinationJNDI>jms/iaTopic</DestinationJNDI>
						</Port>
					</ListenerPorts>
				</MessageListenerService>
			</Server>
		</Instances>
	</ApplicationServer>
	<ClusterResources>
		<JMS>
			<Provider>
				<Name>SonicMQ</Name>
				<ClassPath>${SONIC_CLASSPATH}</ClassPath>
				<ExtInitCtxFactory>com.sonicsw.jndi.mfcontext.MFContextFactory</ExtInitCtxFactory>
				<ExtProviderURL>${SONIC_DOMAINMGR_URL}</ExtProviderURL>
				<CustomProperties>
					<Property>
						<Name>com.sonicsw.jndi.mfcontext.idleTimeout</Name>
						<Value>6000</Value>
					</Property>
					<Property>
						<Name>com.sonicsw.jndi.mfcontext.domain</Name>
						<Value>KAHDomain</Value>
					</Property>
					<Property>
						<Name>java.naming.security.principal</Name>
						<Value>USGClient</Value>
					</Property>
					<Property>
						<Name>java.naming.security.credentials</Name>
						<Value>USGClient</Value>
					</Property>
				</CustomProperties>
				<ConnectionFactory>
					<Name>IAConnectionFactory</Name>
					<Type>UNIFIED</Type>
					<JNDI>jms/IAConnectionFactory</JNDI>
					<ExtJNDI>USGConnectionFactory</ExtJNDI>
					<ComponentManagedAuthAlias>AgentAlerts_SONIC_CF</ComponentManagedAuthAlias>
				</ConnectionFactory>
				<JMSDestination>
					<Name>IntellAgentQueue</Name>
					<Type>QUEUE</Type>
					<JNDI>jms/iaQueue</JNDI>
					<ExtJNDI>KAH.IntellAgent.Fault</ExtJNDI>
				</JMSDestination>
				<JMSDestination>
					<Name>IntellAgentTopic</Name>
					<Type>TOPIC</Type>
					<JNDI>jms/iaTopic</JNDI>
					<ExtJNDI>KAH.IntellAgent.IntellAgentEmailProcess.MDB</ExtJNDI>
				</JMSDestination>
			</Provider>
		</JMS>
		<JDBCResources>
			<SQLProvider>
				<ProviderName>SQL_JDBC_PROVIDER</ProviderName>
				<DataSource>
					<DataSourceName>AgentPreferences</DataSourceName>
					<DataSourceJNDIName>jdbc/agentalerts</DataSourceJNDIName>
					<ComponentManagedAuthAlias>AgentAlerts_SQL_IntellAgent</ComponentManagedAuthAlias>
					<DataBaseName>IntellAgent</DataBaseName>
					<DataBaseServerName>usgobtsql920a.unitrininc.unitrin.org</DataBaseServerName>
					<PortNumber>1666</PortNumber>
					<ConnectionPoolProperties>
						<ConnectionPoolTimeout>45</ConnectionPoolTimeout>
						<ConnectionPoolMaxConnections>15</ConnectionPoolMaxConnections>
						<ConnectionPoolMinConnections>0</ConnectionPoolMinConnections>
						<ConnectionPoolReapTime>45</ConnectionPoolReapTime>
						<ConnectionPoolUnusedTimeout>45</ConnectionPoolUnusedTimeout>
						<ConnectionPoolagedTimeout>0</ConnectionPoolagedTimeout>
						<ConnectionPoolPurgePolicy>EntirePool</ConnectionPoolPurgePolicy>
					</ConnectionPoolProperties>
					<WASDataSourceProperties>
						<StatementCacheSize>10</StatementCacheSize>
						<PretestSQLString>SELECT @@ SERVERNAME</PretestSQLString>
						<PretestConnectionEnable>NO</PretestConnectionEnable>
					</WASDataSourceProperties>
					<CustomProperties>
						<sendStringParametersAsUnicode>false</sendStringParametersAsUnicode>
					</CustomProperties>
				</DataSource>
			</SQLProvider>
		</JDBCResources>
		<MailResources>
			<MailSessions>
				<MailSessionName>agentalerts</MailSessionName>
				<MailSessionJNDIName>mail/agentalerts</MailSessionJNDIName>
				<MailTransportHost>smtprelay</MailTransportHost>
				<MailTransportUserID></MailTransportUserID>
				<MailTransportPassword></MailTransportPassword>
				<MailFromAddress>NoReply@eCalypso.com</MailFromAddress>
			</MailSessions>
		</MailResources>
		<URLResources>
			<URL>
				<URLName>agentalertsEnvironment.xml</URLName>
				<URLJNDIName>url/agentalertsEnvironment.xml</URLJNDIName>
				<URLSpec>file:///${APP_INSTALL_ROOT}/${WAS_CELL_NAME}/AgentAlerts.ear/AgentAlertsWeb.war/WEB-INF/classes/Environment.${deployEnv}.xml</URLSpec>
			</URL>
			<URL>
