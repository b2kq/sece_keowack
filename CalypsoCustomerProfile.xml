<EnterpriseApplication>
	<ear-info>
		<ear-name>CalypsoCustomerProfile.ear</ear-name>
		<app-name>CalypsoCustomerProfile</app-name>
	</ear-info>
	<mod-info>
		<module>
			<web>
				<war-file-name>CalypsoCustomerPrivate.war</war-file-name>
				<display-name>CalypsoCustomerPrivate</display-name>
				<virtual-host>default_host</virtual-host>
				<server-name>wwwint-eCalypso-com</server-name>
			</web>
		</module>
		<module>
			<web>
				<war-file-name>CalypsoCustomerPublic.war</war-file-name>
				<display-name>CalypsoCustomerPublic</display-name>
				<virtual-host>default_host</virtual-host>
				<server-name>wwwint-eCalypso-com</server-name>
			</web>
		</module>
		<module>
			<ejb>
				<ejb-file-name>CalypsoCustomerEJB.jar</ejb-file-name>
				<display-name>CalypsoCustomerEJB</display-name>
			</ejb>
		</module>
	</mod-info>
	<Security>
		<J2CAuthData>
			<J2C>
				<Alias>CalypsoCustomerProfile_UDB_KCWSINT</Alias>
				<Username>CalypsoCUSTAPP</Username>
				<Password>Noj@56er</Password>
			</J2C>
		</J2CAuthData>
	</Security>
	<Node>
		<NodeName>was730aINT64V70</NodeName>
		<NodeName>was730bINT64V70</NodeName>
	</Node>
	<ApplicationServer>
		<ClusterName>CalypsoCustomerProfile</ClusterName>
		<Instances>
			<Server>
				<HAManager>
					<Enabled>true</Enabled>
				</HAManager>
				<EndPoints>
					<PortItem>
						<PortName>WC_defaulthost</PortName>
						<Port>9142</Port>
					</PortItem>
				</EndPoints>
				<WebContainer>
					<SessionManagement>
						<CookieName>KCPJSESSIONID</CookieName>
						<SessionTimeout>15</SessionTimeout>
						<!--DistributedSessionType>NONE</DistributedSessionType-->
						<DistributedSessionType>DATA_REPLICATION</DistributedSessionType>
						<ReplicationDomain>CalypsoCustomerProfile</ReplicationDomain>
						<ReplicationMode>BOTH</ReplicationMode>
					</SessionManagement>
				</WebContainer>
				<JavaVirtualMachine>
					<VerboseGarbageCollection>TRUE</VerboseGarbageCollection>
					<InitialHeapSize>256</InitialHeapSize>
					<MaximumHeapSize>768</MaximumHeapSize>
				</JavaVirtualMachine>
				<MonitoringPolicy>
					<maximumStartupAttempts>3</maximumStartupAttempts>
					<pingInterval>60</pingInterval>
					<pingTimeout>300</pingTimeout>
					<autoRestart>true</autoRestart>
					<serverRestartState>RUNNING</serverRestartState>
				</MonitoringPolicy>
				<JVMLogs>
					<SystemOut>
						<JVMLogsSystemOutFileFormat>BASIC</JVMLogsSystemOutFileFormat>
						<JVMLogsSystemOutRolloverType>BOTH</JVMLogsSystemOutRolloverType>
						<JVMLogsSystemOutNumberOfBackupFiles>5</JVMLogsSystemOutNumberOfBackupFiles>
						<JVMLogsSystemOutRolloverSize>50</JVMLogsSystemOutRolloverSize>
						<JVMLogsSystemOutBaseHour>24</JVMLogsSystemOutBaseHour>
						<JVMLogsSystemOutRolloverPeriod>24</JVMLogsSystemOutRolloverPeriod>
						<JVMLogsSystemOutFormatWrites>true</JVMLogsSystemOutFormatWrites>
						<JVMLogsSystemOutsuppresWrites>false</JVMLogsSystemOutsuppresWrites>
						<JVMLogsSystemOutsuppressStackTrace>false</JVMLogsSystemOutsuppressStackTrace>
					</SystemOut>
					<SystemError>
						<JVMLogsSystemErrorFileFormat>BASIC</JVMLogsSystemErrorFileFormat>
						<JVMLogsSystemErrorRolloverType>BOTH</JVMLogsSystemErrorRolloverType>
						<JVMLogsSystemErrorNumberOfBackupFiles>5</JVMLogsSystemErrorNumberOfBackupFiles>
						<JVMLogsSystemErrorRolloverSize>50</JVMLogsSystemErrorRolloverSize>
						<JVMLogsSystemErrorBaseHour>24</JVMLogsSystemErrorBaseHour>
						<JVMLogsSystemErrorRolloverPeriod>24</JVMLogsSystemErrorRolloverPeriod>
						<JVMLogsSystemErrorFormatWrites>true</JVMLogsSystemErrorFormatWrites>
						<JVMLogsSystemErrorsuppresWrites>false</JVMLogsSystemErrorsuppresWrites>
						<JVMLogsSystemErrorsuppressStackTrace>false</JVMLogsSystemErrorsuppressStackTrace>
					</SystemError>
				</JVMLogs>
			</Server>
		</Instances>
	</ApplicationServer>
	<ClusterResources>
		<JDBCResources>
			<DB2Provider>
				<ProviderName>DB2_JDBC_PROVIDER</ProviderName>
				<DataSource>
					<DataSourceName>CustomerDataSource</DataSourceName>
					<DataSourceJNDIName>jdbc/cwsds</DataSourceJNDIName>
					<ComponentManagedAuthAlias>CalypsoCustomerProfile_UDB_KCWSINT</ComponentManagedAuthAlias>                   
					<DataBaseName>KCWSINT</DataBaseName>
					<DataBaseDriverType>4</DataBaseDriverType>          
					<DataBaseServerName>kahobtudb90.kah.Calypso.com</DataBaseServerName>
					<PortNumber>3711</PortNumber>                  
					<ConnectionPoolProperties>              
						<ConnectionPoolTimeout>45</ConnectionPoolTimeout>       
						<ConnectionPoolMaxConnections>15</ConnectionPoolMaxConnections>
						<ConnectionPoolMinConnections>0</ConnectionPoolMinConnections>
						<ConnectionPoolReapTime>45</ConnectionPoolReapTime>      
						<ConnectionPoolUnusedTimeout>45</ConnectionPoolUnusedTimeout> 
						<ConnectionPoolagedTimeout>0</ConnectionPoolagedTimeout>   
						<ConnectionPoolPurgePolicy>EntirePool</ConnectionPoolPurgePolicy>
					</ConnectionPoolProperties>   
					<WASDataSourceProperties>
						<StatementCacheSize>10</StatementCacheSize>                   
						<PretestSQLString>SELECT 1 FROM SYSIBM.SYSDUMMY1</PretestSQLString>
						<PretestConnectionEnable>NO</PretestConnectionEnable>
					</WASDataSourceProperties>
					<CustomProperties>
						<resultSetHoldability>1</resultSetHoldability>
					</CustomProperties>
				</DataSource>
			</DB2Provider>
		</JDBCResources>
		<MailResources>
			<MailSessions>
				<MailSessionName>customersmtp</MailSessionName>
				<MailSessionJNDIName>mail/customersmtp</MailSessionJNDIName>
				<MailTransportHost>smtprelay</MailTransportHost>
				<MailTransportUserID></MailTransportUserID>
				<MailTransportPassword></MailTransportPassword>
				<MailFromAddress>CustomerService@eCalypso.com</MailFromAddress>
			</MailSessions>
		</MailResources>
		<URLResources>
			<URL>
				<URLName>custPrivateEnvironment.xml</URLName>
				<URLJNDIName>url/custPrivateEnvironment.xml</URLJNDIName>
				<URLSpec>file:///${APP_INSTALL_ROOT}/${WAS_CELL_NAME}/CalypsoCustomerProfile.ear/config/Environment.${deployEnv}.xml</URLSpec>
			</URL>
			<URL>
				<URLName>custPrivateProperty.xml</URLName>
				<URLJNDIName>url/custPrivateProperty.xml</URLJNDIName>
				<URLSpec>file:///${APP_INSTALL_ROOT}/${WAS_CELL_NAME}/CalypsoCustomerProfile.ear/config/Property.xml</URLSpec>
			</URL>
			<URL>
				<URLName>custPublicEnvironment.xml</URLName>
				<URLJNDIName>url/custPublicEnvironment.xml</URLJNDIName>
				<URLSpec>file:///${APP_INSTALL_ROOT}/${WAS_CELL_NAME}/CalypsoCustomerProfile.ear/config/Environment.${deployEnv}.xml</URLSpec>
			</URL>
			<URL>
				<URLName>custPublicProperty.xml</URLName>
				<URLJNDIName>url/custPublicProperty.xml</URLJNDIName>
				<URLSpec>file:///${APP_INSTALL_ROOT}/${WAS_CELL_NAME}/CalypsoCustomerProfile.ear/config/Property.xml</URLSpec>
			</URL>
		</URLResources>
		<NameSpaceBindings>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>outagemsg</NameSpaceIdentifier>
				<NameSpaceName>config/outagemsg</NameSpaceName>
				<NameSpaceStringValue>Our system is down for maintenance.</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>outage</NameSpaceIdentifier>
				<NameSpaceName>config/outage</NameSpaceName>
				<NameSpaceStringValue>false</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>IDMCustomerAppID</NameSpaceIdentifier>
				<NameSpaceName>config/IDMCustomerAppID</NameSpaceName>
				<NameSpaceStringValue>KAHZCW</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>IDMLDAPCustomerServer0</NameSpaceIdentifier>
				<NameSpaceName>config/IDMLDAPCustomerServer0</NameSpaceName>
				<NameSpaceStringValue>ldap://ldap1-int.Calypsoi.com:12345/</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>IDMLDAPCustomerServer1</NameSpaceIdentifier>
				<NameSpaceName>config/IDMLDAPCustomerServer1</NameSpaceName>
				<NameSpaceStringValue>ldap://ldap2-int.Calypsoi.com:12345/</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>IDMCustomerPublic</NameSpaceIdentifier>
				<NameSpaceName>config/IDMCustomerPublic</NameSpaceName>
				<NameSpaceStringValue>http://kahobtwas71.kah.Calypso.com:9103/idm/TEWS6/customerpublic</NameSpaceStringValue>
			</NameSpace>
			<NameSpace>
				<NameSpaceType>String</NameSpaceType>
				<NameSpaceIdentifier>IDMCustomerPrivate</NameSpaceIdentifier>
				<NameSpaceName>config/IDMCustomerPrivate</NameSpaceName>
				<NameSpaceStringValue>http://kahobtwas71.kah.Calypso.com:9103/idm/TEWS6/customeridm</NameSpaceStringValue>
			</NameSpace>
		</NameSpaceBindings>
	</ClusterResources>
</EnterpriseApplication>